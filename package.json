{
	"name": "lib",
	"version": "0.0.1",
	"scripts": {
		"build": "npm run sass:all && npm run compile",
		"clean": "./bin/setup-dist",
		"compile": "./bin/tsc",
		"dev:ui": "npm run watch & npm run watch-sass:ui & npm run start:ui & wait",
		"lint": "tslint --project tsconfig.json",
		"lint:headless": "tslint --project test/headless/tsconfig.json",
		"lint:webdriver": "tslint --project test/webdriver/tsconfig.json",
		"nuke": "rm -rf dist node_modules",
		"postinstall": "./bin/setup-dist",
		"sass:all": "npm run sass:ui",
		"sass:ui": "node-sass src/ui/assets/styles/main.scss dist/ui/assets/styles/main.css --output-style compressed",
		"start:all": "npm run start:ui",
		"start:ui": "cd dist/ui && node --icu-data-dir=../../node_modules/full-icu ../node_modules/ui/server.js",
		"test": "find dist -name '*.spec.js' | NODE_ICU_DATA=node_modules/full-icu xargs node_modules/mocha/bin/mocha --timeout 10000 --exit",
		"watch": "./bin/tsc -w",
		"watch-sass:ui": "node-sass -w -r src/ui/assets/styles -o dist/ui/assets/styles --output-style compressed"
	},
