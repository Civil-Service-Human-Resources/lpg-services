{
	"name": "lib",
	"version": "0.0.1",
	"scripts": {
		"build": "concurrently \"npm run compile\" \"npm run sass\"",
		"compile": "tsc",
		"dev:ui": "concurrently \"npm run watch-sass\"  \"npm run start:dev\"",
		"lint": "concurrently \"npm run eslint\" \"npm run format\"",
		"fix-lint": "concurrently \"npm run fix-eslint\" \"npm run fix-format\"",
		"eslint": "eslint",
		"fix-eslint": "eslint --fix",
		"format": "prettier --check \"**/*.+(js|ts|json)\"",
		"fix-format": "prettier --write \"**/*.+(js|ts|json)\"",
		"nuke": "rm -rf dist node_modules",
		"sass": "sass views/assets/styles/main.scss:views/assets/styles/main.css --style compressed",
		"start": "node dist/server.js",
		"start:build": "npm run build && npm run start",
		"start:dev": "nodemon src/server.ts",
		"start:ts": "npm run compile && npm run start",
		"test": "concurrently \"npm run test:ui\" \"npm run test:unit\"",
		"test:ui": "npm run test-single:ts \"test/**/*.spec.ts\"",
		"test:unit": "npm run test-single:ts \"src/**/*.spec.ts\"",
		"test-single:ts": "ts-mocha --paths -p ./tsconfig.json --timeout 10000 --exit",
		"test:watch": "ts-mocha src/**/*.spec.ts --paths -w -watch-files '**/*.ts'",
		"watch-sass": "sass --watch views/assets/styles/main.scss:views/assets/styles/main.css --style compressed"
	},
